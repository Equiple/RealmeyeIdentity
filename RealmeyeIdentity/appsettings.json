{
  "UserDatabase": {
    "Database": "realmeyeIdentity",
    "UserCollectionName": "users"
  },
  "Password": {
    "SaltLengthBytes": 32,
    "HashLengthBytes": 128,
    "HashMemoryKbytes": 19456,
    "HashIterations": 2,
    "HashParallelismDegree": 1
  },
  "RegistrationSession": {
    "LifetimeMinutes": 10,
    "IdLengthBytes": 64
  },
  "CodeGenerator": {
    "CodeLengthBytes": 8
  },
  "IdToken": {
    "Issuer": "RealmeyeIdentity",
    "LifetimeMinutes": 15
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
